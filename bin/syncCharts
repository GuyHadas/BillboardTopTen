require 'json'

charts = JSON.parse(File.read("public/alternative/11-23-2016/billboard-data-alternative-11-23-2016.json"))
track_meta = JSON.parse(File.read("track-meta-alternative-11-23-2016.json"))

charts.each do |k, v|
  charts.delete(k) unless track_meta[k]
end

File.write("./public/alternative/11-23-2016/billboard-data-alternative-11-23-2016-synced.json", JSON.generate(charts))
